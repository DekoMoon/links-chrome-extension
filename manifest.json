{
  "manifest_version": 2,
  "name": "Links",
  "version": "1.0",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+L"
      },
      "description": "Open Popup"
    }
  },
  "browser_action": {
    "default_icon": {
      "128": "img/label-icon-128.png"
    },
    "default_title": "Links",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/label-icon-128.png"
  },
  "permissions": ["tabs", "storage"]
}